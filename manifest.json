{
  "name": "Vivocha On The Fly",
  "version": "1.2",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "tabs", "*://*/*"
  ],
  "browser_action": {
    "name": "Vivocha On The Fly",
    "default_icon": "favicon.ico",
