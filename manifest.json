{
  "name": "Vivocha On The Fly",
  "version": "2.0.0-alpha",
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "permissions": [
    "tabs"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "browser_action": {
    "name": "Vivocha On The Fly",
    "default_icon": "small.ico",
    "default_popup": "index.html"
  },
  "icons": {
    "16":  "small.ico",
    "48":  "img/medium.png",
    "128": "img/big.png"
  },
  "manifest_version": 3
}
